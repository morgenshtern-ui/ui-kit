{
  "name": "@teleskop-labs/ui-kit-utils",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "type-check": "vue-tsc --noEmit -p ./tsconfig.vitest.json --composite false",
    "test": "pnpm run test:unit:run",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:run": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "prettier": "prettier --check --ignore-unknown \"**/*\"",
    "prettier:fix": "prettier --write --ignore-unknown \"**/*\"",
    "eslint": "eslint \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\"",
    "eslint:fix": "eslint --fix \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,vue}\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules"
  },
  "dependencies": {
    "@teleskop-labs/ui-kit-constants": "workspace:*",
    "@vue/shared": "3.3.4",
    "@vueuse/core": "10.4.0",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@types/jsdom": "21.1.2",
    "@types/lodash-es": "4.17.8",
    "@vitest/coverage-v8": "0.34.3",
    "@vitest/ui": "0.34.3",
    "expect-type": "0.16.0",
    "jsdom": "22.1.0",
    "type-fest": "4.2.0",
    "vitest": "0.34.3"
  }
}
